<h1>Welcome to dictionary, <%= @user.login %>!</h1>
<p> <%= link_to "My Profile", user_path %> </p>
<p> <%= link_to "Logout", user_session_path, :method => :delete %> </p>
<p>
  <p>Supported languages:</p>
	<% $langs.each do |lang| %>
    <tp> <%= "#{lang},\t" %> </tp>
  <% end %>
</p>

<%= render partial: "interpretation_request_form"%>

<%= render partial: "interpretations", locals: {title: "Interpretations: ", interpretations: @translations} %>

<%= render partial: "interpretations", locals: {title: "History of interpretations: ", interpretations: @history} %>